	&lt;li style=&quot;color:#BEBEC5;&quot;&gt;
		&lt;span style=&quot;color:#48484C;&quot;&gt;$redis &lt;/span&gt;&lt;span style=&quot;color:#93A1A1;&quot;&gt;=&lt;/span&gt;&lt;span style=&quot;color:#48484C;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;color:#1E347B;&quot;&gt;new&lt;/span&gt;&lt;span style=&quot;color:#48484C;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;color:teal;&quot;&gt;Redis&lt;/span&gt;&lt;span style=&quot;color:#93A1A1;&quot;&gt;();&lt;/span&gt;&lt;span style=&quot;color:#48484C;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;color:#93A1A1;&quot;&gt;//实例化redis对象&lt;/span&gt;
