		&lt;span style=&quot;color:#48484C;&quot;&gt;$redis &lt;/span&gt;&lt;span style=&quot;color:#93A1A1;&quot;&gt;-&amp;gt;&lt;/span&gt;&lt;span style=&quot;color:#48484C;&quot;&gt; connect&lt;/span&gt;&lt;span style=&quot;color:#93A1A1;&quot;&gt;(&lt;/span&gt;&lt;span style=&quot;color:#DD1144;&quot;&gt;\'localhost\'&lt;/span&gt;&lt;span style=&quot;color:#93A1A1;&quot;&gt;,&lt;/span&gt;&lt;span style=&quot;color:#DD1144;&quot;&gt;\'6379\'&lt;/span&gt;&lt;span style=&quot;color:#93A1A1;&quot;&gt;);&lt;/span&gt;&lt;span style=&quot;color:#48484C;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;color:#93A1A1;&quot;&gt;//连接服务&lt;/span&gt;
	&lt;/li&gt;

	&lt;li style=&quot;color:#BEBEC5;&quot;&gt;
		&lt;span style=&quot;color:#48484C;&quot;&gt;$redis &lt;/span&gt;&lt;span style=&quot;color:#93A1A1;&quot;&gt;-&amp;gt;&lt;/span&gt;&lt;span style=&quot;color:#48484C;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;color:#1E347B;&quot;&gt;select&lt;/span&gt;&lt;span style=&quot;color:#93A1A1;&quot;&gt;(&lt;/span&gt;&lt;span style=&quot;color:#195F91;&quot;&gt;0&lt;/span&gt;&lt;span style=&quot;color:#93A1A1;&quot;&gt;);&lt;/span&gt;&lt;span style=&quot;color:#48484C;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;color:#93A1A1;&quot;&gt;//具体操作&lt;/span&gt;
