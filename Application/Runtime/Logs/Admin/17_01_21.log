		&lt;span style=&quot;color:#48484C;&quot;&gt;$redis &lt;/span&gt;&lt;span style=&quot;color:#93A1A1;&quot;&gt;-&amp;gt;&lt;/span&gt;&lt;span style=&quot;color:#48484C;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;color:#1E347B;&quot;&gt;set&lt;/span&gt;&lt;span style=&quot;color:#93A1A1;&quot;&gt;(&lt;/span&gt;&lt;span style=&quot;color:#DD1144;&quot;&gt;\'key\'&lt;/span&gt;&lt;span style=&quot;color:#93A1A1;&quot;&gt;,&lt;/span&gt;&lt;span style=&quot;color:#DD1144;&quot;&gt;\'value\'&lt;/span&gt;&lt;span style=&quot;color:#93A1A1;&quot;&gt;);&lt;/span&gt;
