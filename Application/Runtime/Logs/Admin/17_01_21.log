		&lt;span style=&quot;color:#48484C;&quot;&gt;slaveof &lt;/span&gt;&lt;span style=&quot;color:#195F91;&quot;&gt;192.168&lt;/span&gt;&lt;span style=&quot;color:#93A1A1;&quot;&gt;.&lt;/span&gt;&lt;span style=&quot;color:#195F91;&quot;&gt;40.148&lt;/span&gt;&lt;span style=&quot;color:#48484C;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;color:#195F91;&quot;&gt;6379&lt;/span&gt;
